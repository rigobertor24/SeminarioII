#include<bits/stdc++.h>
	using namespace std;
	

	int cost[100][100] , n; 
	

	int getMin(int dist[] , bool visited[]){
	    int key = 0; 
	    int min = INT_MAX; 
	    
	    }

	for(int i=0;i < n ; i++){
	        if(!visited[i] && dist[i]<min){
	            min = dist[i] ; 
	            key = i ; 
	        }
	    }
	    return key ; 
	 }


